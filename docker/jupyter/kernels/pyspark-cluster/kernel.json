{
  "display_name": "Pyspark (cluster)",
  "language": "python",
  "argv": [
    "/usr/bin/python3",
    "-m",
    "ipykernel_launcher",
    "-f",
    "{connection_file}"
  ],
  "env": {
    "SPARK_HOME": "${SPARK_HOME}",
    "PYTHONPATH": "${SPARK_HOME}/python",
    "PYSPARK_DRIVER_PYTHON": "jupyter",
    "PYSPARK_PYTHON": "python3",
    "PYTHONSTARTUP": "${PYTHON_KERNELS_PATH}/pyspark-cluster/init.py"
  }
}
